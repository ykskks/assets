# 環境変数
export your_env_var="xxx"
export PATH="/path/to/something:$PATH"

# alias
alias your_alias="xxx"

# autocorrectしない
unsetopt correct_all
unsetopt correct
DISABLE_CORRECTION="true"

# git alias
alias gs="git status"
alias ga="git add"
alias gb="git branch"
alias gc="git commit -m"
alias gp="git push"
alias gsw="git switch"